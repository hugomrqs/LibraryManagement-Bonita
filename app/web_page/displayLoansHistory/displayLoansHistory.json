    "userLoansHistory" : {
      "type" : "expression",
      "value" : [
        "return $data.getAllLoans.filter(function(loan){",
        "    return loan.userId === $data.userSession.user_id",
        "})"
      ],
      "exposed" : false
    },
    "getAllLoans" : {
      "type" : "businessdata",
      "value" : [
        "{\"displayValue\":\"find [com.company.model.LoanBuisinessObject]\",\"businessObjectName\":\"LoanBuisinessObject\",\"query\":{\"name\":\"find\",\"displayName\":\"find\"},\"id\":\"com.company.model.LoanBuisinessObject\",\"filters\":[],\"pagination\":{\"p\":\"0\",\"c\":\"10\"}}"
      ],
      "exposed" : false
    },
    "userSession" : {
      "type" : "url",
      "value" : [
        "../API/system/session/unusedId"
      ],
      "exposed" : false
    },
    "returnedUserLoansNumber" : {
      "type" : "expression",
      "value" : [
        "let x = $data.userLoansHistory // Supposons que getAllLoans() renvoie un tableau d'objets",
        "let returnedloanedCount = 0;",
        "",
        "x.forEach(loan => {",
        "    if (!loan.isLoaned) {",
        "        returnedloanedCount++;",
        "    }",
        "});",
        "",
        "return returnedloanedCount;"
      ],
      "exposed" : false
    },
    "currentUserLoansNumber" : {
      "type" : "expression",
      "value" : [
        "let x = $data.userLoansHistory // Supposons que getAllLoans() renvoie un tableau d'objets",
        "let loanedCount = 0;",
        "",
        "x.forEach(loan => {",
        "    if (loan.isLoaned) {",
        "        loanedCount++;",
        "    }",
        "});",
        "",
        "return loanedCount;"
      ],
      "exposed" : false
    }
