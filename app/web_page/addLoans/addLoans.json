    "available_books" : {
      "type" : "businessdata",
      "value" : [
        "{\"displayValue\":\"findByIsAvailable [com.company.model.BookBusinessObject]\",\"businessObjectName\":\"BookBusinessObject\",\"query\":{\"name\":\"findByIsAvailable\",\"displayName\":\"findByIsAvailable\"},\"id\":\"com.company.model.BookBusinessObject\",\"filters\":[{\"name\":\"isAvailable\",\"type\":\"BOOLEAN\",\"collection\":false,\"value\":\"true\"}],\"pagination\":{\"p\":\"0\",\"c\":\"10\"}}"
      ],
      "exposed" : false
    },
    "getAllLoans" : {
      "type" : "businessdata",
      "value" : [
        "{\"displayValue\":\"find [com.company.model.LoanBuisinessObject]\",\"businessObjectName\":\"undefined\",\"query\":{\"name\":\"find\",\"displayName\":\"find\"},\"id\":\"com.company.model.LoanBuisinessObject\",\"filters\":[],\"pagination\":{\"p\":\"0\",\"c\":\"10\"}}"
      ],
      "exposed" : false
    },
    "userLoans" : {
      "type" : "expression",
      "value" : [
        "return $data.getAllLoans.filter(function(loan){",
        "    return loan.isLoaned === true && loan.userId === $data.user.user_id",
        "})"
      ],
      "exposed" : false
    },
    "currentLoansNumber" : {
      "type" : "expression",
      "value" : [
        "let x = $data.userLoans // Supposons que userLoans() renvoie un tableau d'objets",
        "let loanedCount = 0;",
        "",
        "x.forEach(loan => {",
        "    if (loan.isLoaned) {",
        "        loanedCount++;",
        "    }",
        "});",
        "",
        "return loanedCount;"
      ],
      "exposed" : false
    }
