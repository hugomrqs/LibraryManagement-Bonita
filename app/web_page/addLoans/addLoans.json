  "variables" : {
    "formInput" : {
      "type" : "json",
      "value" : [
        "{",
        "  \"loanInput\" : [ ]",
        "}"
      ],
      "exposed" : false
    },
    "submit_errors_list" : {
      "type" : "expression",
      "value" : [
        "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){",
        "\tconst liElements = $data.formOutput._submitError.explanations",
        "\t\t.filter(cause => cause !== null)",
        "\t\t.map(cause => \"<li>\" + cause + \"</li>\")",
        "\t\t.join('');",
        "\tif(liElements){",
        "\t\treturn \"<ul>\" + liElements + \"</ul>\";",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "let x = $data.formInput.loanInput;",
        "let y =x.map(book => ({",
        "  userId: 0,",
        "  loanDate: null,",
        "  returnDate: null,",
        "  isLoaned: false,",
        "  bookId: {",
        "    persistenceId: book.persistenceId,",
        "    persistenceId_string: book.persistenceId_string,",
        "    persistenceVersion: book.persistenceVersion,",
        "    persistenceVersion_string: book.persistenceVersion_string,",
        "    title: book.title,",
        "    author: book.author,",
        "    isAvailable: book.isAvailable",
        "  }",
        "}));",
        "return {",
        "    loanInput: y,",
        "    currentLoans: $data.userLoans.length",
        "}"
      ],
      "exposed" : false
    },
    "bookBusinessObject_query" : {
      "type" : "url",
      "value" : [
        "../API/bdm/businessData/com.company.model.BookBusinessObject?q=find&p=0&c=99&f=isAvailable=true"
      ],
