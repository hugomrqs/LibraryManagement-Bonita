  "variables" : {
    "formInput" : {
      "type" : "json",
      "value" : [
        "{",
        "  \"returnLoanInput\" : [ ]",
        "}"
      ],
      "exposed" : false
    },
    "submit_errors_list" : {
      "type" : "expression",
      "value" : [
        "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){",
        "\tconst liElements = $data.formOutput._submitError.explanations",
        "\t\t.filter(cause => cause !== null)",
        "\t\t.map(cause => \"<li>\" + cause + \"</li>\")",
        "\t\t.join('');",
        "\tif(liElements){",
        "\t\treturn \"<ul>\" + liElements + \"</ul>\";",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "return {",
        "\treturnLoanInput: $data.formInput.returnLoanInput",
        "}"
      ],
      "exposed" : false
    },
    "userLoans" : {
      "type" : "expression",
      "value" : [
        "return $data.getAllLoans.filter(function(loan){",
        "    return loan.isLoaned === true && loan.userId === $data.userSession.user_id",
        "})"
      ],
      "exposed" : false
    },
    "userSession" : {
      "type" : "url",
      "value" : [
        "../API/system/session/unusedId"
      ],
      "exposed" : false
    },
    "getAllLoans" : {
      "type" : "businessdata",
      "value" : [
        "{\"displayValue\":\"find [com.company.model.LoanBuisinessObject]\",\"businessObjectName\":\"LoanBuisinessObject\",\"query\":{\"name\":\"find\",\"displayName\":\"find\"},\"id\":\"com.company.model.LoanBuisinessObject\",\"filters\":[],\"pagination\":{\"p\":\"0\",\"c\":\"10\"}}"
      ],
      "exposed" : false
    },
    "currentLoansNumber" : {
      "type" : "expression",
      "value" : [
        "let x = $data.userLoans // Supposons que getAllLoans() renvoie un tableau d'objets",
        "let loanedCount = 0;",
        "",
        "x.forEach(loan => {",
        "    if (loan.isLoaned) {",
        "        loanedCount++;",
        "    }",
        "});",
        "",
        "return loanedCount;"
      ],
      "exposed" : false
    }
